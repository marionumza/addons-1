<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="seq_absence_ref" model="ir.sequence">
        <field name="name">Default Absence ID Seq</field>
        <field name="code">absence.default.seq</field>
        <field name="prefix">AB/%(y)s/</field>
        <field name="padding">5</field>
    </record>

    <record id="seq_suspension_ref" model="ir.sequence">
        <field name="name">Default Suspension ID Seq</field>
        <field name="code">suspension.default.seq</field>
        <field name="prefix">SU/%(y)s/</field>
        <field name="padding">5</field>
    </record>

</odoo>
